// before
// expect(GcpFormMock).toHaveBeenCalledWith(
//   expect.objectContaining({ validationErrors }),
//   expect.anything()
// );

// after
expect(GcpFormMock).toHaveBeenCalledWith(
  expect.objectContaining({ validationErrors })
);
expect(CdmNextFormMock).toHaveBeenCalledWith(
  expect.objectContaining({ validationErrors })
);

// (optional: if the form might render more than once, assert the last call)
expect(GcpFormMock).toHaveBeenLastCalledWith(
  expect.objectContaining({ validationErrors })
);
expect(CdmNextFormMock).toHaveBeenLastCalledWith(
  expect.objectContaining({ validationErrors })
);
