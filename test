function makeLogs(overall: 'success' | 'failure' = 'success'): Logs {
  return {
    overall_status: overall,
    validation_results: {
      // âœ… correct key name
      gcpapp_validation: {
        status: 'success',
        details: [] as any[], // keep as array to match type
      },
      gcpdm_validation: {
        status: 'success',
        details: [] as any[],
      },
      github_repos_validation: {
        status: 'success',
        details: [] as any[],
      },
      secondary_ranges_validation: {
        status: 'success',
        details: [] as any[],
      },
      workspace_validation: {
        status: 'success',
        details: [] as any[],
      },
    },
  };
}
